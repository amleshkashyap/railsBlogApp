<h1>Welcome</h1>

<% if logged_in? %>
  <h4>You are Logged In, <%= current_user.name %>, <%= current_user.email %></h4>
<% else %>
  <%= button_to "Login", '/login', method: :get%>
  <br><br>
  <%= button_to "Sign Up", '/users/new', method: :get%>
  <br><br>
<% end %>
<%= link_to "Home", root_path %>
